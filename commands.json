    {
        "id": 13,
        "name": "Purge",
        "Note": "The purge command makes use of a flag system for different options. \nFor example, \"purge 10 --user Geek --match oof\" would purge the last 10 messages from the user Geek if it contains oof",
        "commands": {
            "purge": {
                "Description": "Bulk delete messages",
                "Usage": "{prefix}purge <amount> [<flags>]",
                "Aliases": "prune"
            }
	},
	"flags": {
            "user": {
                "Description": "Purge messages from this user",
                "Usage": "--user <name/mention/id>"
            },
            "channel": {
                "Description": "Purge messages in this channel",
                "Usage": "--channel <name/mention/id>"
            },
            "match": {
                "Description": "Purge messages containing a string",
                "Usage": "--match <string>"
            },
            "nomatch": {
                "Description": "Purge messages that do not contain a string",
                "Usage": "--nomatch <string>"
            },
            "startswith": {
                "Description": "Purge messages starting with a string",
                "Usage": "--startswith <string>"
            },
            "endswith": {
                "Description": "Purge messages ending with a string",
                "Usage": "--endswith <string>"
            },
            "attachments": {
                "Description": "Purge messages with attachments (true = with, false = without)",
                "Usage": "--attachments <true/false>"
            },
            "bot": {
                "Description": "Purge messages from bots (true = bot, false = not bot)",
                "Usage": "--bot <true/false>"
            },
            "invite": {
                "Description": "Purge messages containing invites (true = with invites, false = without invites)",
                "Usage": "--invite <true/false>"
            },
            "text": {
                "Description": "Purge messages containing only text (true = only text, false = attachments, embeds etc.)",
                "Usage": "--text <true/false>"
            }
        }
    },
