    {
        "id": 10,
        "name": "Moderation",
        "Note": "Users with the \"Manage Messages\" permission are considered moderators! This will most likely change in the future.",
        "commands": {
            "ban": {
                "Description": "Ban a user from the server",
                "Usage": "{prefix}ban <user> [<reason>]",
                "Aliases": "banish, begone, gtfo, 410"
            },
            "unban": {
                "Description": "Unban a user from the server",
                "Usage": "{prefix}unban <user> [<reason>]",
                "Aliases": "unbanish"
            },
            "softban": {
                "Description": "Bans a user, deletes messages from a specified amount of days and then unbans.",
                "Usage": "{prefix}softban <user> <amount of days: 1-7> [<reason>]",
                "Aliases": null
            },
            "mute": {
                "Description": "Mute a user.",
                "Usage": "{prefix}mute <user> [<time> <reason>] Time format: 1d 2h 3m 4s == 1 day, 2 hours, 3 minutes and 4 seconds",
                "Aliases": "silence, tempmute, 403"
            },
            "unmute": {
                "Description": "Unmute a muted user.",
                "Usage": "{prefix}unmute <user>",
                "Aliases": null
            },
            "warn": {
                "Description": "Warn a user.",
                "Usage": "{prefix}warn <user> <reason>",
                "Aliases": null
            },
            "warnings": {
                "Description": "View warnings for a user",
                "Usage": "{prefix}warnings <user>",
                "Aliases": null
            },
            "clearwarns": {
                "Description": "Clear a users warnings",
                "Usage": "{prefix}clearwarns <user>",
                "Aliases": "clearwarnings"
            },
            "clearwarn": {
                "Description": "Clear a single warning",
                "Usage": "{prefix}clearwarn <case id>",
                "Aliases": "clearwarning"
            },
            "modlogs": {
                "Description": "View moderation logs for a user",
                "Usage": "{prefix}modlogs <user>",
                "Aliases": null
            },
            "kick": {
                "Description": "Kick a user.",
                "Usage": "{prefix}kick <user> [<reason>]",
                "Aliases": "yeet, 409"
            },
            "block": {
                "Description": "Mute a user/role in the current channel.",
                "Usage": "{prefix}block <user/role> [<reason>]",
                "Aliases": null
            },
            "unblock": {
                "Description": "Unmute a user/role who has been blocked in the current channel.",
                "Usage": "{prefix}unblock <user/role> [<reason>]",
                "Aliases": null
            },
            "derank": {
                "Description": "Remove all roles from a user (The role must be below Fire's highest role to be removed)",
                "Usage": "{prefix}derank <user> [<reason>]",
                "Aliases": "dethrone"
            }
        }
    },
