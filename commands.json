    {
        "id": 7,
        "name": "Music",
        "commands": {
            "connect": {
                "Description": "Connect to a voice channel",
                "Usage": "{prefix}connect <channel>",
                "Aliases": "join"
            },
            "play": {
                "Description": "Queue a song or playlist for playback.",
                "Usage": "{prefix}play <search query/url>",
                "Aliases": "sing, p"
            },
